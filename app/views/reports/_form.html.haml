= simple_form_for(@report) do |f|
  = f.error_notification

  .form-inputs
    = f.association :user
    = f.association :report_type
    = f.input :report_type_option_ids
    = f.input :name
    = f.input :start_date
    = f.input :end_date
    = f.input :notes
    = f.input :logo_enabled

  .form-actions
    = f.button :submit
